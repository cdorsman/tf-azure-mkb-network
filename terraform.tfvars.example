# Example terraform.tfvars file
# Copy this file to terraform.tfvars and customize the values

# Basic Configuration
resource_group_name = "RG-MKB-Netwerk"
location           = "West Europe"
vnet_name          = "mkb-vnet"

# Optional: Disable Azure Bastion if not needed (saves costs)
enable_bastion = true

# Note: SSH keys are automatically generated by the stress-test module
# No need to manually provide SSH keys anymore